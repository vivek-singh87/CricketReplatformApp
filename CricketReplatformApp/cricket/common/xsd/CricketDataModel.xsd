			<xs:element name="AddressLine2" type="AddressLineInfo"
				default="" minOccurs="0" />
			<xs:element name="AddressLine3" type="AddressLineInfo"
				default="" minOccurs="0" />
			<xs:element name="City">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="State">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Country">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PostalCode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SetFMFInfo">
		<xs:sequence>
			<xs:element name="mdn" type="TelephoneNumberInfo" />
			<xs:element name="accountNumber" type="BillingAccountNumberInfo" />
			<xs:element name="productId" type="xs:string" />
			<xs:element name="quoteId" type="ExternalTransactionIdInfo" />
			<xs:element name="EffectiveDates" type="DateTimeIntervalInfo" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceTargetingInfo">
		<xs:sequence>
			<xs:element name="deviceWithHistory" type="xs:boolean"
				default="false" />
			<xs:element name="isCricketDevice" type="xs:boolean"
				default="true" />
			<xs:element name="isPayGoDevice" type="xs:boolean"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates if the phone is a paygo phone or not.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OfferTriageTaskSetInfo">
		<xs:annotation>
			<xs:documentation>Associates an offerId with a set of triage tasks.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="offerId" type="OfferingCodeInfo" />
			<xs:element name="offeringDescription" type="OfferingDescriptionInfo"
				minOccurs="0" />
			<xs:element name="offerGroupId" type="OfferingCodeInfo"
				minOccurs="0" />
			<xs:element name="offerGroupName" type="OfferingCodeInfo"
				minOccurs="0" />
			<xs:element name="costDisplay" type="xs:string" minOccurs="0" />
			<xs:element name="TaskSet" type="TaskSetInfo" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskSetInfo">
		<xs:annotation>
			<xs:documentation>A group of task comprises a Task Set. Task sets may
				be assigned names or identifiers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:string" minOccurs="0" />
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="description" type="xs:string" minOccurs="0" />
			<xs:choice>
				<xs:element name="Task" type="TaskInfo" minOccurs="0"
					maxOccurs="unbounded" />
				<xs:element name="TaskSetEvaluationResult" type="TaskSetResponseInfo"
					minOccurs="0">
					<xs:annotation>
						<xs:documentation>If the set has already been evaluated, no need
							to re-evaluate. Evaluation result will be echoed back on the
							response. If a RESET is requested, appropriate action will be
							taken to reset prior state. E.g Manager Override of Device
							Condition.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskInfo">
		<xs:annotation>
			<xs:documentation>This is a task. Usually it would be a question or
				action to be taken by a sales rep or system.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:string" minOccurs="0" />
			<xs:element name="name" type="xs:string" minOccurs="0" />
			<xs:element name="description" type="xs:string" minOccurs="0" />
			<xs:element name="Response" type="TaskResponseInfo"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskResponseInfo">
		<xs:annotation>
			<xs:documentation>A task will have a single response of predetermined
				type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="yesNoResponse" type="yesNoResponseInfo" />
				<xs:element name="integerResponse" type="xs:int"
					nillable="true" />
				<xs:element name="decimalResponse" type="xs:double"
					nillable="true" />
				<xs:element name="stringResponse" type="xs:string"
					nillable="true" />
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskSetResponseInfo">
		<xs:annotation>
			<xs:documentation>The result of the task set evaluation.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="PASS" />
						<xs:enumeration value="FAIL" />
						<xs:enumeration value="UNKNOWN" />
						<xs:enumeration value="RESET" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="description" type="xs:string" minOccurs="0"
				maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>May contain the reason(s) for a FAIL response.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaxInfo">
		<xs:annotation>
			<xs:documentation> Simple Tax info relates to charges
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="taxAmount" type="CurrencyAmountInfo" />
			<xs:element name="taxType" type="TaxTypeInfo" minOccurs="0" />
			<xs:element name="taxTypeName" type="xs:string" minOccurs="0" />
			<xs:element name="geoCode" type="TaxGeoCodeInfo"
				minOccurs="0" />
			<xs:element name="description" type="CommentInfo"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="yesNoResponseInfo">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Yes" />
			<xs:enumeration value="No" />
			<xs:enumeration value="Unknown" />
		</xs:restriction>
	</xs:simpleType>
</xs:schema>