	<xs:simpleType name="AccountDiscountActionInfo">
		<xs:annotation>
			<xs:documentation>Simple type for account discount action ADD or
				REMOVE</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ADD" />
			<xs:enumeration value="REMOVE" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BackEndSystemNameInfo">
		<xs:annotation>
			<xs:documentation>An enumeration of the different back end systems
				that ESP integrates with. Values should match service entity names
				in ServiceEntityConfiguration elements.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CVG" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BackEndSystemStatusInfo">
		<xs:annotation>
			<xs:documentation>An enumeration of the different back end systems
				statuses that are possible.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="UP" />
			<xs:enumeration value="DOWN" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UpdateDeviceReasonCodeInfo">
		<xs:annotation>
			<xs:documentation>Reason device was updated. LOST_STOLEN - Update due
				to Lost or Stolen device.			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="LOST_STOLEN" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocationIdInfo">
		<xs:annotation>
			<xs:documentation>Unique identifier created by the DRM system
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="39" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DealerTypeInfo">
		<xs:annotation>
			<xs:documentation>CCMD Dealer Types 1 - Dealer 2 - Internal / Direct
				Store 3 - Payment Location Only 4 - Premier Dealer 5 - Sub-Dealer 6
				- Internet Services Only 7 - Premier with Service			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="DEALER" />
			<xs:enumeration value="INTERNAL_DIRECT_STORE" />
			<xs:enumeration value="PAYMENT_LOCATION_ONLY" />
			<xs:enumeration value="PREMIER_DEALER" />
			<xs:enumeration value="SUB_DEALER" />
			<xs:enumeration value="INTERNET_SERVICES_ONLY" />
			<xs:enumeration value="PREMIER_WITH_SERVICE" />
			<xs:enumeration value="UNKNOWN" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AuditTypeInfo">
		<xs:annotation>
			<xs:documentation> 1 FailedManagerAuthorizationIdOverride 2
				AccountVerification3rdTryFailed 3
				SuccessfulManagerAuthorizationIdOverride 4
				AuthorizationIdAuthorization2ndTryFailed 101
				FailedValidateAPLPaymentAttempts 102 IneligibleDeviceChangeAttempts
				103 IneligibleDeviceActivationAttempts								</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FailedManagerAuthorizationIdOverride" />
			<xs:enumeration value="AccountVerification3rdTryFailed" />
			<xs:enumeration value="SuccessfulManagerAuthorizationIdOverride" />
			<xs:enumeration value="AuthorizationIdAuthorization2ndTryFailed" />
			<xs:enumeration value="FailedValidateAPLPaymentAttempts" />
			<xs:enumeration value="IneligibleDeviceChangeAttempts" />
			<xs:enumeration value="IneligibleDeviceActivationAttempts" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DataMeteringTypeInfo">
		<xs:annotation>
			<xs:documentation> C - Capped, T - Throttled </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1" />
			<xs:enumeration value="C" />
			<xs:enumeration value="T" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ABPServiceActionInfo">
		<xs:annotation>
			<xs:documentation> Actions that the ManageABP services can perform.
				Enumerations: UnEnroll ABP Enroll ABP Enroll Check ABP
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ENROLL_ABP" />
			<xs:enumeration value="ENROLL_CHECK_ABP" />
			<xs:enumeration value="UNENROLL_ABP" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AWSErrorCodeInfo">
		<xs:annotation>
			<xs:documentation> Defines enumeration for AWS error codes ErrorCode
				of 1 means the phone is not defined. Non-AWS phone/datacard cannot
				be activated in AWS market ErrorCode of 3 means that the market is
				not defined. Non Datacard cannot be activated in Evergreen market.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:enumeration value="1" />
			<xs:enumeration value="3" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AWSStatusCodeInfo">
		<xs:annotation>
			<xs:documentation>Defines enumeration for AWS error codes. AWS (voice
				phone) - refers to a handset that is tri-band compatible.
				AWS-capable handsets can only be used in certain markets. K - no
				issues P - the phone is not defined; non-AWS phone/datacard cannot
				be activated in AWS market M - the market is not defined; non
				datacard cannot be activated in Evergreen markets Evergreen refers
				to collection of markets where data cards can be used
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="K" />
			<xs:enumeration value="P" />
			<xs:enumeration value="M" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountArrangementStatusInfo">
		<xs:annotation>
			<xs:documentation>AccountArrangementStatusInfo OK PENDING CLOSED
				CANCELED TERMINATED			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10" />
			<xs:enumeration value="OK" />
			<xs:enumeration value="PENDING" />
			<xs:enumeration value="CLOSED" />
			<xs:enumeration value="CANCELED" />
			<xs:enumeration value="TERMINATED" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountBalanceTypeInfo">
		<xs:annotation>
			<xs:documentation>C=Current Due, T=Total Due, P=Past Due
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1" />
			<xs:enumeration value="C" />
			<xs:enumeration value="T" />
			<xs:enumeration value="P" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountPaymentArrangementTypeInfo">
		<xs:annotation>
			<xs:documentation>Special agreement account payment types BPAY -
				Bridge Pay PEXT - Payment Extension PRSA - Payment Research Special
				Agreement LAHP - Legal Authorities Hotline Prevention.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BPAY" />
			<xs:enumeration value="PEXT" />
			<xs:enumeration value="PRSA" />
			<xs:enumeration value="LAHP" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountProfileStatusInfo">
		<xs:annotation>
			<xs:documentation> Lifecycle status for the PayGo or BBMM accounts:
				ACTIVE, EXPIRED, CANCELLED, DEFICIENT, POST_DISCONNECT,
				MANUAL_DISCONNECT, UNREGISTERED			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ACTIVE" />
			<xs:enumeration value="EXPIRED" />
			<xs:enumeration value="CANCELLED" />
			<xs:enumeration value="DEFICIENT" />
			<xs:enumeration value="POST_DISCONNECT" />
			<xs:enumeration value="MANUAL_DISCONNECT" />
			<xs:enumeration value="UNREGISTERED" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountStatusInfo">
		<xs:annotation>
			<xs:documentation>Active - A, Cancelled - C, Suspended - S, Hotlined
				- H, Unhotlined - U; Pending - P</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1" />
			<xs:enumeration value="A" />
			<xs:enumeration value="C" />
			<xs:enumeration value="S" />
			<xs:enumeration value="H" />
			<xs:enumeration value="U" />
			<xs:enumeration value="P" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountStatusReasonCodeInfo">
		<xs:annotation>
			<xs:documentation>Account Status Reason Info 1. TR - Disconnect on
				Port Out (Default) ; LS - LOST PHONE ; NP - NON PAYMENT ;NS - NON
				SUFFICIENT FUNDS ;ST - STOLEN PHONE ;NS - NSF FEE ;CB - CHARGEBACK ;
				FR - FRAUD ; PD - Paid ;RL - Recovered Lost Phone ;RS - Recovered
				Stolen Phone ;RP - Replaced Broken Phone </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountTypeIndicatorInfo">
		<xs:annotation>
			<xs:documentation> Business - B, Government - G, Individual - I,
				Special - S, Prepaid Account - P, Post-Pay Account - Y, Employee
				Account - E, Jump Account - J, Cricket By Week Account - W, Pay Go
				Account - O, Hargray Account - H, Broadband Mass Merchant - M
				Wholesale Billed - L Generic Shipping - X </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1" />
			<xs:enumeration value="B" />
			<xs:enumeration value="G" />
			<xs:enumeration value="S" />
			<xs:enumeration value="I" />
			<xs:enumeration value="P" />
			<xs:enumeration value="Y" />
			<xs:enumeration value="E" />
			<xs:enumeration value="J" />
			<xs:enumeration value="W" />
			<xs:enumeration value="O" />
			<xs:enumeration value="H" />
			<xs:enumeration value="M" />
			<xs:enumeration value="L" />
			<xs:enumeration value="X" />
