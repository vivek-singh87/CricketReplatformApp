	function initDrawers()
	{
		// Utility Nav Drawers - could try this: http://jsfiddle.net/ProLoser/nurx8/
		$("#li-location a, .close-location").click(function(){
			$(".content-cart, .content-search, .content-log-in").hide();
			$(".content-location").toggle();
			$("#li-log-in a, #li-cart a, #li-search a").removeClass("open");
			$("#li-location a").toggleClass("open");
			$("#drawer-indicator").removeClass();
			$("#drawer-utility-indicator").removeClass("dui-log-in").toggleClass("dui-location");
			//$(".content-location").toggleClass('drawer-slide-up').toggleClass('drawer-slide-down');
			//return false;
		});
	
		$("#li-log-in a, .close-log-in").click(function(){
			$(".content-cart, .content-search, .content-location").hide();
			$(".content-log-in").toggle();
			$("#li-location a, #li-cart a, #li-search a").removeClass("open");
			$("#li-log-in a").toggleClass("open");
			$("#drawer-indicator").removeClass();
			$("#drawer-utility-indicator").removeClass("dui-location").toggleClass("dui-log-in");
		});
			
		// Main Nav Drawers 
		$("#li-cart a, .close-cart").click(function(){
			$(".content-location, .content-log-in, .content-search").hide();
			$(".content-cart").toggle();
			$("#li-location a, #li-log-in a, #li-search a").removeClass("open");
			$("#li-cart a").toggleClass("open");
			$("#drawer-utility-indicator").removeClass();			
			$("#drawer-indicator").removeClass("di-search").toggleClass("di-cart");
		});
		
		$("#li-search a, .close-search").click(function(){	
			$(".content-location, .content-log-in, .content-cart").hide();		
			$(".content-search").toggle();
			$("#li-location a, #li-log-in a, #li-cart a").removeClass("open");
			$("#li-search a").toggleClass("open");
			$("#drawer-utility-indicator").removeClass();
			$("#drawer-indicator").removeClass("di-cart").toggleClass("di-search");
		});		
	
	}	
	
	function getDeviceWidth()
	{
		var w = $(document).width();
		
		if(w<768)
		{
			// This changes footer sitemap of 5 columns to an accordion on small screen
			$("#footer-sitemap").addClass("section-container accordion");

			//$("#header").addClass("sticky fixed");
			
			$("#li-log-in a").text("Log In");
			// Close all drawers if open
			$(".drawer, .drawer-utility").hide();
			$("#drawer-indicator").removeClass();
			$("#drawer-utility-indicator").removeClass();
			//var innerWrapRight = $("#inner-wrap").css("right");
		}	
		else
		{
			
			// Init Drawers for location, login, cart, and search
			initDrawers();
								
			$("#nav-utility").removeClass("mobile-utility");
			$('#inner-wrap').animate({ right: '0' }, 500, function() { $("#constructor").css("border","100px solid red") });
			//$("#header").removeClass("sticky fixed");
			$("#li-log-in a").text("My Account");			
			// This changes back to footer sitemap
			$("#footer-sitemap").removeClass("section-container accordion");
		}	
		
	}
	
	$(function(){

		getDeviceWidth();
		
		$(window).resize(function(){
			getDeviceWidth();				
		});

			
		$('#open-side-menu a').on('click', function(e) {
